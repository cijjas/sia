{
    "problem": {
        "type": "number_identifier",
        "data": "../res/ej3/TP3-ej3-digitos.txt",
        "output": "output/ej3/parity.csv",
        "testing_data": "../res/ej3/TP3-ej3-xor.txt"
    },
    "network": {
        "topology": [35,20,10],
        "activation_function": {
            "method": "sigmoid",
            "beta": 1
        },
        "optimizer": {
            "method": "gradient_descent"
        }
    },
    "training": {
        "seed": 23,
        "epochs": 20,
        "mini_batch_size": 50,
        "learning_rate": 16,
        "epsilon": 0.01,
        "n_splits": 10, "REMEMBER":"currently, the training set is big/TP3-ej3-digitos.txt, which has 1000 digits"
    }
  
}

