{
    "kohonen": {
        "seed": 42,
        "grid_size": 5,
        "learning_rate": 0.1,
        "eta_function": "exponential_decay",
        "initial_radius": 5,
        "radius_function": "exponential_decay",
        "number_of_iterations": 1000,
        "similarity_function": "euclidean"
    },
    "data": {
        "source": "../data/europe.csv",
        "scaling": "standard",
        "features": [
            "Area",
            "GDP",
            "Inflation",
            "Life.expect",
            "Military",
            "Pop.growth",
            "Unemployment"
        ]
    }
}